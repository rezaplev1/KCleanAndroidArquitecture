
CREATE TABLE DCharacterEntity (
  	id	INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
  	name	TEXT NOT NULL DEFAULT "",
  	modified	TEXT  NOT NULL,
  	resourceURI TEXT NOT NULL
);

readAll:
SELECT  *
FROM DCharacterEntity;

readById:
SELECT  *
FROM DCharacterEntity WHERE DCharacterEntity.id = ?;

insert:
INSERT INTO DCharacterEntity(id,name,modified,resourceURI)
VALUES (?,?, ?,?);

